<h1>User::Posts#index</h1>
<p>Find me in app/views/user/posts/index.html.erb</p>
<%= link_to "新規投稿", new_post_path %>
<% @posts.each do |post| %>
  <p>画像</p>
  <%= image_tag post.image, size: "200x200" %>
  <p>ジム名</p>
  <%= post.name %>
  <p>住所</p>
  <%= post.address %>
  <P>紹介文</P>
  <%= post.introduction %>
  <%= link_to "詳細へ", post_path(post.id) %>
<% end %>
