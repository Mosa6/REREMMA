<div class="px-5 mx-5">
  <div class="mx-3"><h3 class="mb-5">管理者トップ</h3></div>
  <div class="row mx-5 px-5">
    <table class="table border-bottom">
      <thead class="thead-light">
        <tr>
          <th>ユーザー名</th>
        </tr>
      </thead>
      <% @users.each do |user| %>
      <tbody>
        <tr>
          <td><%= link_to user.name, admin_user_path(user) %></td>
        </tr>
      </tbody>
      <% end %>
    </table>
  </div>
</div>